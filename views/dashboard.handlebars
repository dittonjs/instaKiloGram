<div class="content">
      <div class="post-rows">
        <div class="row">
          {{#each dashboard}}
          <div class="recent-post-container">
            <div class="inner-container">
              <div class="inner-header">
                <script language="javascript">
	               var jsonDate = {{this.created_time}};
	                var date = new Date(parseInt(jsonDate) * 1000);
  	               document.write(date.getDate() + "/" + (date.getMonth()+1) +"/"+date.getFullYear() + " " + date.getHours() + ":" + date.getMinutes());
                   </script>
                <!-- {{this.created_time}} -->
                <!-- Date:11/11/2011 -->
              </div>
              <div class="post-content">
                  <img src="{{this.images.thumbnail.url}}" class="post-image"/>
              </div>
            </div>
            <div class="likes-button">
              <span class="button">Likes: {{this.likes.count}}</span>
              <span class="button">Comm: {{this.comments.count}}</span>
            </div>
          </div>
          {{/each}}
        </div>

        <!-- these dont do anything right now but might once we do this for real -->
        <div class="tools">
          <div class="tool-item">Recent Comments</div>
          <div class="tool-item">Recent Posts</div>
          <div class="tool-item">Recent Likes</div>
          <div class="tool-item">Followers</div>
        </div>
      </div>

    </div>
